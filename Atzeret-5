using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 1;
            int b = 2;
            


            do
            {
                a = Convert.ToInt32(a * b);
                b++;
                
            }
            while (b <= 5);

            Console.WriteLine("the answer: {0}", a);
